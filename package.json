{
  "name": "frogger",
  "displayName": "frogger",
  "publisher": "Bluesteel Software",
  "description": "jump cursor using single character searching line 'f' in vim",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "key": "cmd+j",
        "command": "frogger.jump"
      },
      {
        "key": "cmd+i",
        "command": "frogger.toggleInsertCursor",
        "when": "froggerFocused"
      },
      {
        "key": "cmd+j",
        "command": "-workbench.action.togglePanel"
      }
    ],
    "commands": [{
      "command": "frogger.jump",
      "title": "Jump to character ..."
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
